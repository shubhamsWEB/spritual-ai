
NODE_ENV=development
PORT=3001


GROQ_API_KEY=groq-api-key
OPENAI_API_KEY=openai-api-key

QDRANT_HOST=qdrant-host

QDRANT_PORT=6333
QDRANT_API_KEY=qdrant-api-key

LOG_LEVEL=info

PDF_PATH=./data/bhagavad_gita.pdf

PORT=3000
HOST=localhost


API_PREFIX=/api
API_VERSION=v1
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100


LOG_LEVEL=info
LOG_FORMAT=json


QDRANT_MEMORY_MODE=false
QDRANT_COLLECTION_NAME=bhagavad_gita_v2
QDRANT_BATCH_SIZE=20
VECTOR_DIMENSIONS=1536
VECTOR_DISTANCE=Cosine
VECTOR_SIZE=4
VECTOR_QUANTIZATION_ENABLED=true
VECTOR_QUANTIZATION_TYPE=binary
VECTOR_QUANTIZATION_RESCORE=true
VECTOR_DEBUG=false
VECTOR_MAX_RETRIES=3


EMBEDDING_MODEL=BAAI/bge-small-en-v1.5
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSIONS=1536
EMBEDDING_NORMALIZE=true
EMBEDDING_CACHE=true
EMBEDDING_RETRY_COUNT=3
EMBEDDING_RETRY_DELAY=1000
EMBEDDING_BATCH_SIZE=20

LLM_PROVIDER=groq
LLM_MODEL=deepseek-r1-distill-llama-70b
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=2048
LLM_MAX_RETRIES=3

RAG_SIMILARITY_TOP_K=3
RAG_TIMEOUT=30000
RAG_CHUNK_SIZE=512
RAG_CHUNK_OVERLAP=50
RAG_RELEVANCE_THRESHOLD=0.3

SUPPORTED_LANGUAGES=en,hi,sa
DEFAULT_LANGUAGE=en
TRANSLATION_PROVIDER=google
RAG_DEBUG=false

PDF_PATH=./data/Bhagavad-Gita.pdf
DOCUMENT_CACHE_ENABLED=true
